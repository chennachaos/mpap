BEGIN RUN_CONTROL




BATCH
!anly,solv,,28,1,1,1,1,1,1
!anly,solv,,28,1,6,1,1,1,1
!anly,solv,,28,1,8,1,1,1,1
anly,solv,,28,1,10,1,1,1,1
chen,data,,28,2,2,2,1
dt,,1.0
wrnd
wait,mous
loop,,0.9999999999,2
  if,mous
    wait,mous
  ndif
  time
  updt
  loop,,15
    wait,mous
    anly,tang,,28,1,1
    if,conv
      prnt,CONVERGED
      xlop
    ndif
    if,divg,,,1000
      xlop
    ndif
  next
  prnt
  if,conv
    vtk,flow,,28,1,1,1,10,1,1,1,0,0,1
    wrnd
    dt,stck
  else
    time,back
    dt,cut
    rset
  ndif
next
end


END RUN_CONTROL




